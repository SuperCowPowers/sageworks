   "source": [
    "# Include Confidence metric\n",
    "hold_out_df[\"predictions\"] = model.predict(hold_out_df[features])\n",
    "hold_out_df[\"confidence\"] = f_spider.confidence_scores(hold_out_df, hold_out_df[\"predictions\"])"
   ]
  },
  {
